public with sharing class Outsoursing {
	public class MyException extends Exception{}
	/****************************************************************************************************
	*	변수
	****************************************************************************************************/
	//wOSs 중 선택된 seq
	public integer SelectSeq{get; set;}
	//전체 저장이 되었는지
	public boolean IsAllSave{
		get{
			if(IsAllSave==null) IsAllSave = false;
			return IsAllSave;
		}
		private set;
	}
	
	/****************************************************************************************************
	*	외주처리 wrapper class
	****************************************************************************************************/
	public class wOutsoursing implements Comparable{
		public integer seq{get; set;}
		public Inventory__c IV{
			get{
				if(IV==null) IV = new Inventory__c();
				return IV;
			}
			set{
				IV = value;
				if(IV.Id<>null) OS.Inventory__c = IV.Id;
			}
		}
		public Outsoursing__c OS{
			get{
				if(OS==null) OS = new Outsoursing__c();
				return OS;
			}
			set;
		}
		
		//1. 바코드입력대기, 2. 외주의뢰 입력대기, 3.입력완료
		public integer state{
			get{
				if(state==null) state = 1;
				return state;
			}
			private set;
		}
		
		public wOutsoursing(integer gseq){
			seq = gseq;
		}
		
		public integer compareTo(Object compareTo){
			wOutsoursing Obj = (wOutsoursing) compareTo;
			if(seq==Obj.seq) return 0;
			if(seq>Obj.seq) return 1;
			return -1;
		}
	}
	
	/****************************************************************************************************
	*	외주처리 wrapper class의 리스트
	****************************************************************************************************/
	private map<integer, wOutsoursing> mapwOSs{
		get{
			if(mapwOSs==null){
				mapwOSs = new map<integer, wOutsoursing>();
				mapwOSs.put(1, new wOutsoursing(1));
			}
			return mapwOSs; 
		}
		private set;
	}
	
	public list<wOutsoursing> wOSs{
		get{
			if(wOSs==null){
				wOSs = mapwOSs.values();
				wOSs.sort();
			}
			return wOSs;
		}
		private set;
	}
	/****************************************************************************************************
	*	바코드 검색
	****************************************************************************************************/
	public void SearchBarcode(){
		try{
			wOutsoursing wOS = mapwOSs.get(SelectSeq);
			
			//현재 리스트에 포함되어 있는지 확인
			for(wOutsoursing B_wOS:wOSs) if(B_wOS.IV.Name==wOS.IV.Name && B_wOS.seq<>SelectSeq) throw new MyException(string.valueOf(B_wOS.seq) + '번째에 이미 등록이 되어 있습니다.');
			
			list<Inventory__c> IVs = [SELECT ID
				, Name
				, Item__c
				, Item__r.Name
				, Account__c
				, Account__r.Name
				, DateManufacture__c
				, CntImportInspection__c
				, CntInventory2__c 
				, Seq__c
				FROM Inventory__c WHERE Name=:wOS.IV.Name];
			if(IVs.size()<1) throw new MyException('해당하는 바코드는 존재하지 않습니다.');
			
			wOS.IV = IVs[0];
			wOS.OS.CntOutSoursing__c = wOS.IV.CntInventory2__c;
			wOS.state = 2;
		}catch (MyException e){
			ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, e.getMessage()));
		}catch(exception e){
			ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, e.getMessage()));
		}
	}
	
	/****************************************************************************************************
	*	입력
	****************************************************************************************************/
	public void RowSave(){
		try{
			wOutsoursing wOS = mapwOSs.get(SelectSeq);
			
			//입력확인
			if(wOS.OS.Type__c==null || wOS.OS.DateOutsoursing__c==null || wOS.OS.Account__c==null || wOS.OS.CntOutSoursing__c==null) throw new MyException('모든 사항은 필수로 입력하셔야 합니다.');
			
			//의뢰수량은 재고 수량보다 높을 수 없다.
			if(wOS.OS.CntOutSoursing__c>wOS.IV.CntInventory2__c) throw new MyException('의뢰수량이 재고수량보다 많습니다.');
			
			wOS.state = 3;
			
			mapwOSs.put(mapwOSs.size() + 1, new wOutsoursing(mapwOSs.size() + 1));
			
			wOSs = null;
		}catch (MyException e){
			ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, e.getMessage()));
		}catch(exception e){
			ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, e.getMessage()));
		}
	}
	
	/****************************************************************************************************
	*	행삭제
	****************************************************************************************************/
	public void RowDelete(){
		try{
			integer mapSize = mapwOSs.size();
			
			for(integer i=SelectSeq; i<mapSize; i++){
				wOutsoursing wOS = mapwOSs.get(i + 1);
				wOS.seq = i;
				mapwOSs.put(i, wOS);
			}
			
			mapwOSs.remove(mapSize);
			wOSs = null;
		}catch (MyException e){
			ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, e.getMessage()));
		}catch(exception e){
			ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, e.getMessage()));
		}
	}
	
	/****************************************************************************************************
	*	취소
	****************************************************************************************************/
	public void RowCancel(){
		try{
			wOutsoursing wOS = mapwOSs.get(SelectSeq);
			wOS.OS = new Outsoursing__c();
			wOS.IV = new Inventory__c();
			wOS.state = 1;
		}catch (MyException e){
			ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, e.getMessage()));
		}catch(exception e){
			ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, e.getMessage()));
		}
	}
	
	/****************************************************************************************************
	*	저장
	****************************************************************************************************/
	public void Save(){
		try{
			List<Outsoursing__c> InsertOSs = new List<Outsoursing__c>();
			for(wOutsoursing wOS:wOSs){
				if(wOS.state == 3){
					InsertOSs.add(wOS.OS);
				}else if(wOS.state == 1){}
				else throw new MyException('입력이 완료되지 않은 항목이 있습니다. 모두 입력 후에 진행하여 주시기 바랍니다.');
			}
			if(InsertOSs.size()<1) throw new MyException('저장할 항목이 없습니다. 확인 후 진행하여 주시기 바랍니다.');
			
			//for(Inventory__c IV:InsertIVs) system.debug('IV.Item__c : ' + IV.Item__c);
			
			insert InsertOSs;
			
			// cIVs에서 입력대기상태인 Row를 삭제한다.
			for(wOutsoursing wOS:wOSs) if(wOS.state == 1) mapwOSs.remove(wOS.Seq);
			
			wOSs = null;
			IsAllSave = true;
		}catch (MyException e){
			ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, e.getMessage()));
		}catch(exception e){
			ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, e.getMessage()));
		}
	}
	
